{
  "name": "registration",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^1.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.3",
    "clsx": "^1.1.1",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.14.0",
    "react-google-recaptcha": "^2.1.0",
    "react-google-recaptcha-v3": "^1.6.1",
    "react-hook-form": "^6.9.4",
    "react-scripts": "^3.4.4",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false node ./scripts/build-non-chunked.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-clean": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --find 'main\\.[^\\.]+\\.js' -v --replace 'main.js' build/static/js/*.js",
    "build-rename-css": "renamer --find 'main\\.[^\\.]+\\.css' -v --replace 'main.css' build/static/css/*.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "renamer": "^2.0.1",
    "rewire": "^5.0.0"
  }
}
